{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM.Iterable",
      "DOM",
      "ES2019"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2019",
  },
  "include": [
    "remix.env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
}